{
  "version": "0.0.1",
  "configurations": [
      {
          "name": "g++ - Build and Debug Active File",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/build/src/lyncean",
          "args": [
              "--command",
              "ls",
              "-hali"
          ],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "build",
          "miDebuggerPath": "/usr/bin/gdb",
          "logging": {
              "trace": true,
              "traceResponse": true,
              "engineLogging": true,
              "programOutput": true,
              "exceptions": true
          },
          "internalConsoleOptions": "openOnSessionStart"
      }
  ]
}
